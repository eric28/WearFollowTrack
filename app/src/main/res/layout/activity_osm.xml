<?xml version="1.0" encoding="utf-8"?>
<eric.esteban28.wearfollowtrack.MySwipeDismissFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        mapbox:mapbox_cameraTargetLat="39.896580"
        mapbox:mapbox_cameraTargetLng="-0.114537"
        mapbox:mapbox_cameraZoom="16"
        mapbox:mapbox_cameraZoomMax="17"
        mapbox:mapbox_cameraZoomMin="14"
        mapbox:mapbox_styleUrl="@string/mapbox_style_satellite"
        mapbox:mapbox_uiRotateGestures="false">

        <Button
            android:id="@+id/buttonStr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:text="Start" />

        <Button
            android:id="@+id/buttonStp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|center"
            android:text="Stop"
            android:visibility="invisible" />

    </com.mapbox.mapboxsdk.maps.MapView>

</eric.esteban28.wearfollowtrack.MySwipeDismissFrameLayout>